<h1>Listing timeline_events</h1>

<table>
  <tr>
    <th>Start</th>
    <th>End</th>
    <th>Title</th>
    <th>Description</th>
    <th>Image</th>
    <th>Link</th>
    <th>Hash</th>
    <th>Timeline</th>
  </tr>

<% @timeline_events.each do |timeline_event| %>
  <tr>
    <td><%=h timeline_event.start %></td>
    <td><%=h timeline_event.end %></td>
    <td><%=h timeline_event.title %></td>
    <td><%=h timeline_event.description %></td>
    <td><%=h timeline_event.image %></td>
    <td><%=h timeline_event.link %></td>
    <td><%=h timeline_event.hash %></td>
    <td><%=h timeline_event.timeline_id %></td>
    <td><%= link_to 'Show', timeline_event %></td>
    <td><%= link_to 'Edit', edit_timeline_event_path(timeline_event) %></td>
    <td><%= link_to 'Destroy', timeline_event, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New timeline_event', new_timeline_event_path %>