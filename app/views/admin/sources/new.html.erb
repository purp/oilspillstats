<h1>New timeline</h1>

<% form_for([:admin, @timeline]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.label :timeline_id %><br />
    <%= f.text_field :timeline_id %>
  </p>
  <p>
    <%= f.label :chronological_order %><br />
    <%= f.text_field :chronological_order %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', admin_timelines_path %>